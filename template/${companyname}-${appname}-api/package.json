{
  "name": "${companyname}-${appname}-api",
  "version": "0.0.1",
  "genetaredBy": "Boilerplate-node-api",
  "boilerplateVersion": "${boilerplateversion}",
  "description": "${companyname}-${appname}-api",
  "main": "index.js",
  "scripts": {
    "generate-git-info": "node-git-info",
    "copy-sources": "rm -rf ./dist && cp -r src/ dist/",
    "build-routes": "tsoa swagger -c ./tsoa.json && tsoa routes -c ./tsoa.json",
    "build-routes-ingress": "tsoa swagger -c ./tsoa.json --basePath /sample/ && tsoa routes -c ./tsoa.json",
    "dev": "npm run generate-git-info && npm run copy-sources && tsc-watch --onSuccess \"node ./dist/frameworks/webserver/express/server.js\"",
    "compile": "npm run generate-git-info && npm run copy-sources && tsc --project ./tsconfig.json",
    "start": "npm run build-routes && npm run dev",
    "test": "jest --colors --noStackTrace --passWithNoTests --runInBand",
    "test:staged": "npm test --findRelatedTests --silent",
    "test:debug": "npm run compile && jest &",
    "test:ci": "npm test -- --coverage --silent",
    "sonar": "npm run test:ci && node analyse.js",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json './src/**/*.ts' --fix",
    "preversion": "npm run compile && npm run test && npm run lint",
    "version:major": "npm version major --force -m 'Updating the project major version %s'",
    "version:minor": "npm version minor --force -m 'Updating the project minor version %s'",
    "version:patch": "npm version patch --force -m 'Updating the project patch version %s'",
    "postversion": "git push && git push origin --tags"
  },
  "keywords": [
    "Aplicação express baseada na arquitetura clean"
  ],
  "author": "${CompanyName}",
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-actuator": "^1.6.1",
    "form-data": "3.0.0",
    "inversify": "5.0.1",
    "map.prototype.tojson": "^0.1.3",
    "moment": "2.24.0",
    "node-fetch": "^2.6.0",
    "node-git-info": "^1.1.0",
    "pino": "4.10.2",
    "reflect-metadata": "0.1.13",
    "request-promise": "4.2.5",
    "swagger-ui-express": "4.1.3",
    "tsc-watch": "^4.2.9",
    "tsoa": "^3.2.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/express-serve-static-core": "^4.17.9",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.2",
    "@types/express-actuator": "^1.6.0",
    "@types/jest": "^26.0.7",
    "@types/moxios": "^0.4.9",
    "@types/node": "13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/pino": "^6.3.0",
    "@types/request-promise": "4.1.45",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "4.1.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-mock-extended": "^1.0.9",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^9.4.3",
    "mock-express-request": "0.2.2",
    "moxios": "^0.4.0",
    "prettier": "1.19.1",
    "sonarqube-scanner": "^2.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-lines-between-class-members": "^1.3.6"
  }
}